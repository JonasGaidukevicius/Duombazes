SELECT *
FROM DARBUOTOJAI 
WHERE GIMIMOMETAI = '1990-08-22'
--------------------------------------
INSERT INTO DARBUOTOJAI (VARDAS, PAVARDE) VALUES ('GEDIMINAS', 'GEDIMAUSKAS')
-----------------------------------
UPDATE DARBUOTOJAI SET PROJEKTONUMERIS=4 WHERE VARDAS ='GEDIMINAS'
--------------------------------------
UPDATE DARBUOTOJAI  SET VARDAS ='Gediminas', PAVARDE = 'Gedimauskas' WHERE VARDAS = 'GEDIMINAS'
--------------------------------------
SELECT * FROM DARBUOTOJAI 
--------------------------------------
DELETE FROM DARBUOTOJAI WHERE VARDAS='Gediminas'
----------------------------------
DELETE FROM DARBUOTOJAI WHERE VARDAS='Gediminas' LIMIT 1
Limit 1 reiskia, kad ištrins vieną pirmą rastą įrašą
----------------------------------
